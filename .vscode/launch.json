  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Attach using Process Id",
      "type": "python",
      "request": "attach",
      "processId": "${command:pickProcess}",
      "justMyCode": true
    },
    {
      "request": "launch",
      "type": "debugpy",
      "name": "Python: Current File (Admin)", // Example name
      "program": "${file}",
      "console": "integratedTerminal",
      "runAsAdministrator": true // Add this line
    }
  ]
}
